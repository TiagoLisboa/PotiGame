var tempo_antigo = Rel√≥gio.milisegundos

calcular_ds (tempo: Inteiro)
	ds = tempo - tempo_antigo
	tempo_antigo := tempo
	retorne ds/100
fim

ler_lista_inteiro(arq: Texto) =	Arquivo.leia(arq)
         .junte(" ")
	       .passe_enquanto(_ == '[')
				 .pegue_enquanto(_ <> ']')
				 .divida(", ")
				 .mapeie(_.inteiro)
