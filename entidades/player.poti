tipo Jogador
	var x, y: Real
	var largura, altura: Inteiro
	var sprite: Imagem
	var vel = 5

	iniciar() fim

	desenhar() 
		sprite.desenhe(x, y, 3)
	fim

	atualizar() 
		se Teclado.TECLA_W ent達o y := y - 1 * vel fim
		se Teclado.TECLA_S ent達o y := y + 1 * vel fim
		se Teclado.TECLA_A ent達o x := x - 1 * vel fim
		se Teclado.TECLA_D ent達o x := x + 1 * vel fim
	fim
fim