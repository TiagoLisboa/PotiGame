tipo Jogador
	var x, y: Real
	var largura, altura: Inteiro
	var sprite: Imagem
	var mapa: Mapa
	var vel = 5

	iniciar() fim

	desenhar() 
		sprite.desenhe(x, y, 3)
	fim

	atualizar(ds: Real) 
		se Teclado.TECLA_W ent達o y := y - (vel * ds) fim
		se Teclado.TECLA_S ent達o y := y + (vel * ds) fim
		se Teclado.TECLA_A ent達o x := x - (vel * ds) fim
		se Teclado.TECLA_D ent達o x := x + (vel * ds) fim

		escreva mapa.checar_posicao(x, y)
	fim
fim